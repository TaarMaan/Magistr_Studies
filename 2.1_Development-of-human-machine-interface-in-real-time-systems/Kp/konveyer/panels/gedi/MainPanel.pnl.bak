float getNormDistribValue(float aa, float bb) {
float sum =0,x;
for(int i=0;i<8;i++)
  sum+=1.0*rand()/RAND_MAX;
  x= (sqrt(2.0)*(bb)*(sum-6.95))/2.0+aa;
if(x<0){x=x*(-1);}
 return floor(x);
}
